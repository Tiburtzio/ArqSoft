<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testParseMVCListViewSubActivity">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name="com.example.testParseMVCListViewSubActivity.TravelPointsApplication"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.testParseMVCListViewSubActivity.MainListActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
           <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.testParseMVCListViewSubActivity.DisplayActivity"
            android:label="@string/title_activity_display" >
        </activity>
        <activity
            android:name="com.example.testParseMVCListViewSubActivity.AddPointActivity"
            android:label="@string/title_activity_addPoint" >
        </activity>
        <activity
            android:name="com.example.testParseMVCListViewSubActivity.AddIngredientActivity"
            android:label="@string/title_activity_addIngredient" >
        </activity>
    </application>

</manifest>